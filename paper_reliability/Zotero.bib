@article{brose_DifferencesBetweenPersonWithinPerson_2015,
  langid = {english},
  title = {Differences in the {{Between}}-{{Person}} and {{Within}}-{{Person Structures}} of {{Affect Are}} a {{Matter}} of {{Degree}}},
  volume = {29},
  issn = {1099-0984},
  doi = {10.1002/per.1961},
  abstract = {This study tested whether the structure of affect observed on the basis of between-person (BP) differences is equivalent to the affect structures that organize the variability of affective states within persons (WP) over time. Further aims were to identify individual differences in the degree of divergence between the WP and BP structure and examine its association to dispositional and contextual variables (neuroticism, extraversion, well-being and stress). In 100 daily sessions, 101 younger adults rated their mood on the Positive and Negative Affect Schedule. Variability of five negative affect items across time was so low that they were excluded from the analyses. We thus worked with a modified negative affect subscale. WP affect structures diverged reliably from the BP structure, with individual differences in the degree of divergence. Differences in the WP structural characteristics and the degree of divergence could be predicted by well-being and stress. We conclude that BP and WP structures of affect are not equivalent and that BP and WP variation should be considered as distinct phenomena. It would be wrong, for example, to conceive of positive and negative affect as independent at the WP level, as suggested by BP findings. Yet, individual differences in WP structural characteristics are related to stable BP differences, and the degree to which individuals' affect structures diverge from the BP structure can provide important insights into intraindividual functioning. Copyright \textcopyright{} 2014 European Association of Personality Psychology},
  journaltitle = {European Journal of Personality},
  date = {2015},
  pages = {55-71},
  keywords = {ergodicity,intraindividual variability,microlongitudinal design,PANAS,positive and negative affect},
  author = {Brose, Annette and Voelkle, Manuel C. and L\"ovd\'en, Martin and Lindenberger, Ulman and Schmiedek, Florian},
  file = {/Users/felix/Bibs und Backup/Zotero/storage/Z57DSGQX/per.html}
}

