@article{schmidt_effects_1994,
  title = {Effects of Social and Physical Variables on Between-Person Visual Coordination},
  volume = {6},
  issn = {1532-6969},
  doi = {10.1207/s15326969eco0603_1},
  abstract = {Between-person visual coordination of rhythmic limb movements was investigated under manipulation of both social and physical variables, using 18 university students. Each member of a pair of Ss oscillated a hand-held pendulum at the same tempo, with both visually coordinating their movements to maintain an alternate phase mode. Ss' social competence was assessed, and they were paired to produce 3 types of combinations: high-high (HH), low-low (LL), and high-low (HL). Mean relative phase angle and its variability were measured as indices of coordination. HH and LL pairs demonstrated more breakdowns in phase locking than did HL pairs. Moreover, the HH pairs, in producing greater fluctuations in relative phase angle for pendulum combinations of equal length, did not seem to use the dynamical strategy for producing stable phase locking that has been widely observed and produced. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
