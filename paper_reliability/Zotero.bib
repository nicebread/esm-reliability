@article{driver_gaze_1999,
  title = {Gaze Perception Triggers Reflexive Visuospatial Orienting},
  volume = {6},
  issn = {1350-6285},
  doi = {10.1080/135062899394920},
  abstract = {Seeks to bring together 2 previously separate research traditions: research on spatial orienting within the visual cueing paradigm and research into social cognition, addressing our tendency to attend in the direction that another person looks. Cueing methodologies from mainstream attention research were adapted to test the automaticity of orienting in the direction of seen gaze. Three studies (with a total of 27 undergraduate Ss) manipulated the direction of gaze in a computerized face, which appeared centrally in a frontal view during a peripheral letter-discrimination task. Experiments 1 and 2 found faster discrimination of peripheral target letters on the side the computerized face gazed towards, even though the seen gaze did not predict target side. This suggests reflexive covert and/or overt orienting in the direction of seen gaze, arising even when an observer has no motivation to orient in this way. Experiment 3 found faster letter discrimination on the side the computerized face gazed towards even when participants knew that target letters were four times as likely on the opposite side. This suggests that orienting can arise in the direction of seen gaze even when counter to intentions. (PsycINFO Database Record (c) 2010 APA, all rights reserved)},
  journaltitle = {Visual Cognition},
  date = {1999-10},
  pages = {509-540},
  keywords = {Social Perception,Attention,Cues,Orienting Responses,cueing with pictorial stimuli featuring directional gaze of target with vs without subject overt attention; automatism & social perceptual aspects of visual orienting responses; college students,Automatism,Pictorial Stimuli},
  author = {Driver, J. and Davis, G. and Ricciardelli, P. and Kidd, P. and Maxwell, E. and Baron-Cohen, S.}
}

