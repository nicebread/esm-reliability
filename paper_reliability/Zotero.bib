}

@article{kline_analysis_2000,
  title = {Analysis of Interaction Terms in Structural Equation Models: {{A}} Non-Technical Demonstration Using the Deviation Score Approach},
  volume = {32},
  issn = {0008-400X},
  doi = {10.1037/h0087107},
  shorttitle = {Analysis of Interaction Terms in Structural Equation Models},
  abstract = {One of the major shortcomings in using structural equation modeling (SEM) data analytic techniques has been the difficulty in handling interaction terms in the modeling process. The issue is that interaction terms that are created by cross-multiplying raw scores result in the matrix of covariances or correlations being singular (there is at least 1 linear dependency in the matrix). The data analyses will not proceed as the matrix is not positive definite. This paper shows a valid and easy way to cope with the problem of interaction terms by using deviation scores or "centred" variables as the interaction terms. The authors expand on the work of L. S. Aiken and S. G. West (1991), which discussed interaction effects within a multiple regression framework, by taking it into the SEM domain for the benefit of users of SEM programs. (PsycINFO Database Record (c) 2010 APA, all rights reserved)},
  journaltitle = {Canadian Journal of Behavioural Science/Revue canadienne des sciences du comportement},
  date = {2000-04},
