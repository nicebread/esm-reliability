  shortjournal = {J Clin Epidemiol},
  date = {2015-09},
  pages = {1046-1058},
  keywords = {Humans,Models; Statistical,Biostatistics,Confounding,Confounding Factors (Epidemiology),Environment-wide association study,Environmental Exposure,Epidemiologic Methods,Model specification,Mortality,Nutrition Surveys,Observational association,Observational Studies as Topic,Phenotype,Risk Assessment,United States,Vibration of effects,Vitamin E},
