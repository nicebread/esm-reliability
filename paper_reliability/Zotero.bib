  keywords = {Models,consumer behavior,Statistical Analysis,Heuristics,multiple regression,misleading data analysis heuristics & moderated multiple regression models & application in consumer behavior research,Heuristic Modeling},
  author = {Irwin, J. R. and McClelland, G. H.}
}

@article{blanton_tests_2006,
