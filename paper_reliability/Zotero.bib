  author = {Grouzet, Frederick M. E. and Kasser, Tim and Ahuvia, Aaron and Dols, Jos\'e Miguel Fern\'andez and Kim, Youngmee and Lau, Sing and Ryan, Richard M. and Saunders, Shaun and Schmuck, Peter and Sheldon, Kennon M.},
