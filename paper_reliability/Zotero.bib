@article{myin-germeys_EcologicalMomentaryInterventions_2016,
  langid = {american},
  title = {Ecological Momentary Interventions in Psychiatry},
  volume = {29},
  issn = {0951-7367},
  doi = {10.1097/YCO.0000000000000255},
  abstract = {Purpose of review~In this review, we discuss feasibility, content, and where possible efficacy of ecological momentary interventions (EMIs) in psychiatry. EMIs adopt mobile devices, such as personal digital assistants or smartphones, for the delivery of treatments in the daily life of patients. We will discuss EMIs in the field of schizophrenia, bipolar disorder and major depression disorder, as well as one generic, transdiagnostic EMI.
        Recent findings~The few studies that are available all underscore feasibility and acceptability of mobile health approaches in patients with severe mental illness. In terms of content, there is a huge variety in approaches ranging from a mixture of face-to-face contacts augmented with EMI components to a fully automated EMI. With regard to efficacy, only two randomized clinical trials have been conducted, supporting the efficacy of EMIs in mental health. Evidence seems to point toward greater efficacy when EMI is integrated with real-life assessment using experience sampling methodology, preferentially tailoring the intervention toward the specific needs of the individual as well as toward those moments when intervention is needed.
        Summary~The review demonstrates that mobile health may be an important asset to the mental health field but underscores that it still is in its very early ages. In the discussion, we point toward ways of improving EMIs for severe mental illness, changing our perspective from testing feasibility to testing efficacy and ultimately implementing EMIs in routine mental health services.},
  journaltitle = {Current Opinion in Psychiatry},
  date = {2016-07},
  pages = {258},
  author = {Myin-Germeys, Inez and Klippel, Annelie and Steinhart, Henrietta and Reininghaus, Ulrich},
  file = {/Users/felix/Bibs und Backup/Zotero/storage/JFZ5FAVC/Ecological_momentary_interventions_in_psychiatry.6.html}
}

