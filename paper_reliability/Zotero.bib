  keywords = {cognition,Subjectivity,phenomenology,Person Environment Fit,person-environment fit,percieved fit,subjective fit,subjective experience},
  author = {Edwards, J. R. and Cable, D. M. and Williamson, I. O. and Lambert, L. S. and Shipp, A. J.}
}

