@article{hubbard_widespread_2011,
  title = {The Widespread Misinterpretation of P-Values as Error Probabilities},
  volume = {38},
  issn = {0266-4763},
  doi = {10.1080/02664763.2011.567245},
  abstract = {The anonymous mixing of Fisherian (p-values) and Neyman\textendash{}Pearsonian ({$\alpha$} levels) ideas about testing, distilled in the customary but misleading p {$<$} {$\alpha$} criterion of statistical significance, has led researchers in the social and management sciences (and elsewhere) to commonly misinterpret the p-value as a `data-adjusted' Type I error rate. Evidence substantiating this claim is provided from a number of fronts, including comments by statisticians, articles judging the value of significance testing, textbooks, surveys of scholars, and the statistical reporting behaviours of applied researchers. That many investigators do not know the difference between p's and {$\alpha$}'s indicates much bewilderment over what those most ardently sought research outcomes\textemdash{}statistically significant results\textemdash{}means. Statisticians can play a leading role in clearing this confusion. A good starting point would be to abolish the p {$<$} {$\alpha$} criterion of statistical significance.},
  journaltitle = {Journal of Applied Statistics},
  date = {2011},
  pages = {2617-2626},
  author = {Hubbard, R.}
}

