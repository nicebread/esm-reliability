  pages = {208-219},
  keywords = {friendships,childhood,cognitive ability,homophily,personality,social networks},
  author = {Ilmarinen, Ville-Juhani and Vainikainen, Mari-Pauliina and Verkasalo, Markku Johannes and L\"onnqvist, Jan-Erik},
  file = {/Users/felix/Bibs und Backup/Zotero/storage/JE2YFTQD/abstract.html}
}

@article{boelePersonGroupDissimilarity2017,
