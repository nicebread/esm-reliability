  issn = {0027-8424},
  doi = {10.1073/pnas.0902253106},
  abstract = {Many behavioral traits and most brain disorders are common to males and females but are more evident in one sex than the other. The control of these subtle sex-linked biases is largely unstudied and has been presumed to mirror that of the highly dimorphic reproductive nuclei. Sexual dimorphism in the reproductive tract is a product of Mu\textasciidieresis{} llerian inhibiting substance (MIS), as well as the sex steroids. Males with a genetic deficiency in MIS signaling are sexually males, leading to the presumption that MIS is not a neural regulator. We challenge this presumption by reporting that most immature neurons in mice express the MIS-specific receptor (MISRII) and that male Mis[sup]-/-[/sup] and Misrii[sup]-/-[/sup] mice exhibit subtle feminization of their spinal motor neurons and of their exploratory behavior. Consequently, MIS may be a broad regulator of the subtle sex-linked biases in the nervous system. (PsycINFO Database Record (c) 2010 APA, all rights reserved) (journal abstract)},
  journaltitle = {PNAS Proceedings of the National Academy of Sciences of the United States of America},
  date = {2009-04},
  pages = {7203-7208},
  keywords = {behavior,sex-linked biases,brain,anti-Mullerian,Mullerian inhibiting substance,hormone,motor neurons,sexual dimorphism,mice,Animal Ethology,Animal Sex Differences,Hormones,Neurochemistry,Morphology,Nervous System},
  author = {Wang, P. Y. and Protheroe, A. and Clarkson, A. N. and Imhoff, F. and Koishi, K. and McLennan, I. S.}
}

