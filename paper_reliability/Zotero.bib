@article{kromrey_mean_1998,
  langid = {english},
  title = {Mean {{Centering}} in {{Moderated Multiple Regression}}: {{Much Ado}} about {{Nothing}}},
  volume = {58},
  issn = {0013-1644, 1552-3888},
  doi = {10.1177/0013164498058001005},
